<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AppMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AppMAUI"
    xmlns:local2="clr-namespace:AppMAUI.ModelView"
    Shell.FlyoutBehavior="Disabled"
    >

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Login}"
        Route="Login" />

    <ShellContent
        Title="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"/>


    <!--ShellContent
        Title="Employe"
        ContentTemplate="{DataTemplate local:EmployeePage}"
        Route="EmployeeDetail"/>-->


</Shell>
